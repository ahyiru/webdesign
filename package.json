{
  "name": "webdesign",
  "version": "0.0.1",
  "main": "script/index.js",
  "scripts": {
    "start": "nodemon scripts/index.js --watch scripts/index.js",
    "build": "NODE_ENV=production webpack --config scripts/webpack.production.js --progress && npm run clean && mv _dist build",
    "clean": "rimraf build",
    "test": "npm run lint && jest",
    "lint": "eslint 'app/**/*.{js,jsx}'",
    "precommit": "npm run lint"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "modulePathIgnorePatterns": [
      "/build/"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "node"
    ],
    "transform": {
      "^.+\\.tsx?$": "typescript-babel-jest",
      "^.+\\.jsx?$": "babel-jest"
    },
    "collectCoverage": true,
    "testRegex": "(/__tests__/.*\\.(test|spec))\\.(ts|tsx|js|jsx)$"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ahyiru/phoenix-boilerplate"
